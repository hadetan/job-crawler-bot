# Google Custom Search API
GOOGLE_API_KEY=your_api_key_here
GOOGLE_SEARCH_ENGINE_ID=your_search_engine_id_here
SEARCH_QUERY=site:boards.greenhouse.io

# Crawler Settings
CONCURRENCY=5
MAX_PAGES=10
HEADLESS=true
PAGE_TIMEOUT=30000
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36

# Retry Configuration
MAX_RETRIES=3
RETRY_DELAY=2000

# Output
OUTPUT_DIR=./output

# Selectors (Stage 2 - Job Links Extraction)
JOB_LINK_SELECTORS=a[href*="/jobs/"],a[href*="/job/"],a[href*="/careers/"]

# Note: Stage 3 uses intelligent content extraction algorithms
# No manual selector configuration needed - it automatically detects job content
